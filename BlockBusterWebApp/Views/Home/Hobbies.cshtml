@{
    ViewData["Title"] = "Hobbies";
}
<h1>@ViewData["Title"]</h1>
<p>My favorite hobbies are: </p>
<ul>
    @foreach (var hobbyList in ViewBag.Hobbies)
    {
        <li>@hobbyList</li>
    }
</ul>