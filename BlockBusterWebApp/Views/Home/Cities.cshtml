@{
    ViewData["Title"] = "Cities";
}
<h1>@ViewData["Title"]</h1>
<p>The following list are some colors: </p>
<ul>
    @foreach (var cityList in ViewBag.Cities)
    {
        <li>@cityList</li>
    }
</ul>